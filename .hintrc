{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "overscroll-behavior",
          "overscroll-behavior: none",
          "user-select"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "iframe[referrerpolicy=no-referrer-when-downgrade]",
          "video[playsinline]",
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ]
  }
}